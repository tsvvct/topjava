<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:context="http://www.springframework.org/schema/context"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
         http://www.springframework.org/schema/beans/spring-beans.xsd
         http://www.springframework.org/schema/context https://www.springframework.org/schema/context/spring-context.xsd">

    <!--    override бина, нужно чтобы этот файл конфигурации был после spring-app.xml или spring-cache.xml-->
    <bean id="ehCacheManager" class="org.springframework.cache.support.NoOpCacheManager"/>

    <beans profile="jpa,datajpa">
        <context:property-override location="classpath:db/override.properties"/>
    </beans>
</beans>
